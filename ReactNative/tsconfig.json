{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "jsx": "react"
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "__mocks__/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx",
        "src/**/*.json",
        "*.tsx",
        "*.jsx",
        "*.js",
        "src/store/SearchPageProvider/__tests__/mock.js",
        "src/context/InstaCashContext.js"
    ],
    "exclude": [
        "node_modules",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js",
        "e2e/**/*.js",
        "react-native.config.js"
    ],
    "extends": "@tsconfig/react-native/tsconfig.json"
}
